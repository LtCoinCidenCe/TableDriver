{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5192"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7098;http://localhost:5192"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "DATABASEURL": "mySQLDatabase",
        "APPSECRET": "secher"
      },
      "publishAllPorts": true,
      "useSSL": true
    },
    "migrations add": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef migrations add BlogInstance",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      }
    },
    "database update": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef database update",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      }
    },
    "migration bundle": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef migrations bundle",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      }
    },
    "run efbundle.exe": {
      "commandName": "Executable",
      "executablePath": "$(ProjectDir)\\efbundle.exe",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost",
        "APPSECRET": "secher"
      }
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56187",
      "sslPort": 44335
    }
  }
}